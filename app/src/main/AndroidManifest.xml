<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dalin.my.walletbuddy" >


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:name="com.dalin.my.walletbuddy.ParseApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dalin.my.walletbuddy.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dalin.my.walletbuddy.RegisterActivity"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name=".logInActivity"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name="com.dalin.my.walletbuddy.ResetActivity"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name=".HomePage"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name="com.dalin.my.walletbuddy.HomeActivity"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name="com.dalin.my.walletbuddy.TransactionListActivity"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name="com.dalin.my.walletbuddy.BudgetCategoryActivity"
            android:label="WalletBuddy" >
        </activity>
        <activity
            android:name="com.dalin.my.walletbuddy.SetupActivity"
            android:label="WalletBuddy" >
        </activity>

    </application>

</manifest>
